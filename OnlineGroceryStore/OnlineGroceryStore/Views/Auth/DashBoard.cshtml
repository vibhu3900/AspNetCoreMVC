                    <li><a asp-action="index" asp-controller="Products">Products</a></li>
                    <li><a asp-action="index" asp-controller="Categorys">Category</a></li>
                    <li><a href="#">Order</a></li>
                    <li><a href="#">Contact</a></li>
                </ul>
                <ul class="nav navbar-nav navbar-right" asp-controller="Auth" asp-action="AdminLogin">
                    <li><a asp-controller="Auth" asp-action="AdminLogin"><span class="glyphicon glyphicon-user"></span> Your Account</a></li>
                    <li><a href="#"><span class="glyphicon glyphicon-shopping-cart"></span> Cart</a></li>
                </ul>
            </div>
        </div>
    </nav>


    <div class="jumbotron">
        <div class="container text-left">
            <h1>Welcome</h1>
            <h2>Online Grocery Store</h2>
            <p>Mission, Vission & Values</p>
            <div>
                <img src="~/Image/gc.jpg" alt="..." class="navbar-right" width="100%" height="100%" />
            </div>
        </div>
    </div>

